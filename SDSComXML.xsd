					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="SymptomsAndEffectsGeneral" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="AcuteSymptomsAndEffects" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Acute symptoms and effects: Information on </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="DelayedSymptomsAndEffects" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Delayed symptoms and effects: Information on delayed symptoms </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="MedicalAttentionAndSpecialTreatmentNeeded">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Medical attention and special treatment needed: Information if immediate medical attention or special medical treatment is needed after exposure to the chemical. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="MedicalTreatment" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Medical treatment: Information on </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="RequiredClinicalTesting" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Required clinical testing: Information on the need of clinical testing. </xs:documentation>
							</xs:annotation>
