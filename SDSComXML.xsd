						<xs:element maxOccurs="unbounded" minOccurs="0" name="RequiredMedicalMonitoring" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Required medical monitoring: Information on </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="SpecificAntidotes" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Specific antidotes: Information on </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="Contraindications" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Contraindications: Information on </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="SpecificFirstAidEquipment" type="SDScom:Phrase">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Specific first aid equipment: Information about specific equipment that must be available at the work place for instantaneous specific treatment in case of an accident. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="FirstAidComments" type="SDScom:Phrase">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> First aid comments: For instance information to first aid staff or medical information. Indicate whether professional assistance by a doctor is needed or advisable. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Section 5 ========================================================== -->
	<xs:complexType name="FireFightingMeasures">
		<xs:annotation>
			<xs:documentation>SDS section 5</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="ExtinguishingMedia">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="MediaToBeUsed" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Suitable extinguishing media: Extinguishing media that can be used. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="MediaNotBeUsed" type="SDScom:Phrase">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Unsuitable media: Media that must not be applied in case of a fire. </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="FireAndExplosionHazards" type="SDScom:Phrase">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Fire and explosion hazards: Elements of risk in case of a fire or explosion, for instance a particular risk of exposure to the chemical or its decomposition products. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="HazardCombustionProd" type="SDScom:Phrase">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Hazardous combustion products: Information on hazardous components that may be formed during combustion of the chemical. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="FireFightingPrecautions" type="SDScom:Phrase">
				<xs:annotation>
					<xs:documentation> Precautionary measures in case of fire fighting: Measures and special protective equipment for fire-fighters. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="FireFightingProcedures" type="SDScom:Phrase">
				<xs:annotation>
					<xs:documentation> Fire fighting procedures: Information on how to handle a fire in the chemical. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="SpecialProtectiveEquipmentForFirefighters" type="SDScom:Phrase"> </xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="FireAndExplosionComments" type="SDScom:Phrase">
				<xs:annotation>
					<xs:documentation> Comments on fire and explosion information: Additional information and comments to any of the elements in the section. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Section 6 ========================================================== -->
	<xs:complexType name="AccidentalReleaseMeasures">
		<xs:annotation>
			<xs:documentation>SDS section 6</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="ForNonEmergencyPersonnel">
				<xs:annotation>
					<xs:documentation> For non-emergency personnel: </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="PersonalPrecautions" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Personal precautions for non-emergency personnel: Advice on protective equipment etc. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="ProtectiveEquipment" type="SDScom:Phrase"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="EmergencyProcedures" type="SDScom:Phrase"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="ForEmergencyResponders" type="SDScom:Phrase">
				<xs:annotation>
					<xs:documentation> For emergency responders: Information on personal protection, suitable fabric for personal protective clothing, etc. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="EnvironmentalPrecautions" type="SDScom:Phrase">
				<xs:annotation>
					<xs:documentation> Environmental precautions: Information on preventive measures to be taken to protect environment. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="ContainmentAndCleaningUp">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="Containment" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Containment: Advice on how to contain a spill. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="CleaningUp" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Cleaning up: Ways of cleaning-up spills, for instance by application of absorbing material, and the application of water to dilute effluent or restrict emission of gas or fume. Description of cleaning-up methods may be divided into small and large spills (specifications must be made by the individual SDS preparer). </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="OtherInformation" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Other information: Any other information relating to spills and releases, including advice on inappropriate containment or clean up techniques. </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="ReferenceToOtherSections" type="SDScom:Phrase">
				<xs:annotation>
					<xs:documentation> Reference to other sections: If appropriate, reference should be made to information in for instance section 8 and 13. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="AdditionalInformation" type="SDScom:Phrase">
				<xs:annotation>
					<xs:documentation> Additional information: Additional information on any accidental release measures. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Section 7 ========================================================== -->
	<xs:complexType name="HandlingAndStorage">
		<xs:annotation>
			<xs:documentation>SDS section 7</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<!-- Precautions for safe handling -->
			<xs:element minOccurs="0" name="SafeHandling">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="HandlingPrecautions" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Handling precautions: Recommendations to allow safe handling of the chemical, such as containment etc. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="SafeHandlingOfGasContainers" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Safe handling of gas containers: Recommendations and instructions on handling of gas botles and containers to prevent acidents and damages. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="PrecautionaryMeasures">
							<xs:complexType>
								<xs:sequence>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="ProtectiveMeasures" type="SDScom:Phrase"/>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="MeasuresToPreventFire" type="SDScom:Phrase"/>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="MeasuresToPreventAerosolAndDust" type="SDScom:Phrase"/>
									<xs:element maxOccurs="unbounded" minOccurs="0" name="MeasuresToProtectEnvironment" type="SDScom:Phrase"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="FurtherInformation" type="SDScom:Phrase"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="Comments" type="SDScom:Phrase"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="GeneralOccupationalHygiene" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> General occupational hygiene: Advice on general occupational hygiene, such as not to eat, drink and smoke in work areas, to wash hands after use, and to remove contaminated clothing and protective equipment before entering eating areas. </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="StoragePrecautions" type="SDScom:Phrase">
				<xs:annotation>
					<xs:documentation> Storage precautions: Advice on specific storage requirements. How to manage possible risks associated with explosive atmospheres, corrosive conditions, flammability hazards, incompatible chemicals, evaporative conditions, and potential ignition sources. Advices may, if relevant, include ventilation requirements, specific designs for storage rooms or vessels, and quantity limits under storage conditions. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="ConditionsToAvoid" type="SDScom:Phrase">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Conditions to avoid: Materials that must not get in contact with each other during storage etc. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="ConditionsForSafeStorage">
				<xs:annotation>
					<xs:appinfo>SDScomChem</xs:appinfo>
					<xs:documentation> Conditions for safe storage: Information on how to control the effects of weather conditions, ambient pressure, temperature, sunlight, humidity, vibration etc. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="TechnicalMeasuresAndStorageConditions" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Technical measures and storage conditions: Conditions required for safe storage of the chemical. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="PackagingContainer" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Packaging materials: Material and type of container to be used and packaging compabilities. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="RequirementsForStorageRoomsAndVessels" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Requirements for storage rooms and vessels: Specific design for storage rooms or vessels (including retention walls and ventilation), incompatible materials, conditions of storage (temperature and humidity limit/range, light, inert gas, etc.) special electrical equipment and prevention of static electricity. </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="HintsOnStorageAssembly" type="SDScom:Phrase"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="FurtherInformationOnStorageConditions" type="SDScom:Phrase"/>
						<xs:element minOccurs="0" name="StorageTemperature" type="SDScom:PhysChemUnitValue">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Storage temperature: </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="StoragePressure" type="SDScom:PhysChemUnitValue"/>
						<xs:element minOccurs="0" name="StorageAirHumidity" type="SDScom:PhysChemUnitValue"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="StorageStability" type="SDScom:StorageStability">
							<xs:annotation>
								<xs:appinfo>SDScomChem</xs:appinfo>
								<xs:documentation> Storage stability: Information on the use of stabilisers or antioxidants that are required to maintain the integrity of the chemical </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element minOccurs="0" name="SpecificEndUses">
				<xs:annotation>
					<xs:documentation> Specific use: For substances and mixtures designed for specific end use(s), recommendations shall relate to the identified use(s) and be detailed and operational. If industry or sector specific guidance is available, detailed reference to it (including source and issuing date) may be made. </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="Recommendations" type="SDScom:Phrase"/>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="IndustrialSectorSpecificSolutions" type="SDScom:Phrase">
							<xs:annotation>
								<xs:documentation> Industrial sector specific solutions: Information on industrial sector specific solutions. </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Section 8 ========================================================== -->
	<xs:complexType name="ExposureControlPersonalProtection">
		<xs:annotation>
			<xs:documentation>SDS section 8. Also used in "Component" </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="PreventiveIndustrialMedicalExaminationRequired" type="SDScom:Phrase"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ControlParameters">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="1" minOccurs="0" name="OccupationalExposureLimits ">
							<xs:annotation>
								<xs:documentation>Occupational exposure limit: Values shall be given for the country where the substance or preparation is placed on the market. Give information on currently recommended monitoring procedures. For mixtures, it is useful to provide values for those constituent substances which are required to be listed in the Safety Data Sheet. </xs:documentation>
							</xs:annotation>
