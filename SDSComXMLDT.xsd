	<!-- ******************************************************************* PackingGroupEnum *************  -->
	<xs:simpleType name="TransportClass">
		<xs:annotation>
			<xs:documentation>The word "class" is used differently for different carriers. This type denote a class with subclass, but without any categories (specified with letters after the subclass), i.e. it can represent "1" as well as "1.3", but not "1.3G" as used in ADR and ADN. This type can be used for transportation calsses and subrisks.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d(\.\d)?"/>
		</xs:restriction>
	</xs:simpleType>
