	<!-- ******************************************************************** Percentage********  -->
	<xs:simpleType name="Percentage">
		<xs:annotation>
			<xs:documentation>Percentage value 0-100.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:float">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ******************************************************************* ReasonForWaiving *************  -->
	<xs:simpleType name="ReasonForWaiving">
		<xs:annotation>
			<xs:documentation>For physical or chemical properties, reasons for waiving are: 1. cannot be determined = technically impossible to obtain (pH value for solids, arguably) 2. not applicable = irrelevant (because no hazard assessment requires such info) 3. no data = unknown (not tested so far) Implemented for https://github.com/esdscom/sdscom-xml/issues/33</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="cannot be determined"/>
			<xs:enumeration value="not applicable"/>
			<xs:enumeration value="no data"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ******************************************************************* PercentageValue *************  -->
	<xs:complexType name="PercentageValue">
		<xs:annotation>
			<xs:documentation>Basically a UnitValue with a fixed unit "%"</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SDScom:Value"/>
		</xs:complexContent>
	</xs:complexType>
	<!-- ******************************************************************* PhysChemValue***********  -->
	<xs:complexType name="PhysChemValue">
		<xs:annotation>
			<xs:documentation> PhysChemValue, used to specify a value derived from a duration-independent measurement. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element name="Value" type="SDScom:Value" minOccurs="0"/>
					<xs:element name="Method" type="SDScom:Phrase" minOccurs="0"/>
					<xs:element name="TestReference" type="SDScom:Phrase" minOccurs="0"/>
				</xs:sequence>
				<xs:element name="ReasonForWaiving" type="SDScom:ReasonForWaiving" minOccurs="0"/>
			</xs:choice>
			<xs:element name="Comments" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* PhysChemUnitValue *************************  -->
	<xs:complexType name="PhysChemUnitValue">
		<xs:annotation>
			<xs:documentation> PhysChemUnitValue, used to specify a value with unit derived from a duration-independent measurement. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element name="UnitValue" type="SDScom:UnitValue" minOccurs="0"/>
					<xs:element name="Method" type="SDScom:Phrase" minOccurs="0"/>
					<xs:element name="TestReference" type="SDScom:Phrase" minOccurs="0"/>
				</xs:sequence>
				<xs:element name="ReasonForWaiving" type="SDScom:ReasonForWaiving" minOccurs="0"/>
			</xs:choice>
			<xs:element name="Comments" type="SDScom:Phrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* PhysChemValueWithTemperature **************  -->
	<xs:complexType name="PhysChemValueWithTemperature">
		<xs:annotation>
			<xs:documentation> PhysChemValueWithTemperature. Source: Chapter 9. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SDScom:PhysChemValue">
				<xs:sequence minOccurs="0">
					<xs:element name="Temperature" type="SDScom:UnitValue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>defaults to 20°C if not specified, unless the context determines a different value</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Pressure" type="SDScom:UnitValue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>defaults to 1013 hPa if not specified, unless the context determines a different value</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ******************************************************************* PhysChemUnitValueWithTemperature ***********  -->
	<xs:complexType name="PhysChemUnitValueWithTemperature">
		<xs:annotation>
			<xs:documentation> PhysChemUnitValueWithTemperature. Source: Chapter 9. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SDScom:PhysChemUnitValue">
				<xs:sequence minOccurs="0">
					<xs:element name="Temperature" type="SDScom:UnitValue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>defaults to 20°C if not specified, unless the context determines a different value</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Pressure" type="SDScom:UnitValue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>defaults to 1013 hPa if not specified, unless the context determines a different value</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
